<%= simple_form_for([:frontend, @model]) do |f| %>
  <%= f.simple_fields_for :entity do |fi| %>
    <%= fi.input :name, label: 'Nome' %>
    <%= fi.input :email, label: 'Email' %>
  <% end %>
  <%= f.input :password, as: :password %>
  <%= f.input :password_confirmation, as: :password %>
  <%= f.button :submit, class: 'btn btn-primary' %>
<% end %>
